<%- include('./partials/head')%>
<header>
    <a href="/"> Terug</a>
    <progress id="file" value="32" max="100"> 32% </progress>
</header>

<main>
    <form>
        <fieldset>
            <legend>Wie zijn jouw stakeholders?</legend>
            <i>Je hebt stakeholders nodig om de totale...</i>
            <section>
                <h2>Financiers</h2>
                <i>info financiers, bijvoorbeeld...</i>
                <label>
                    Financier 1
                    <select required>
                    <option value=""> ... </option>
                    </select>  
                </label>
            </section>
            <section>
                <h2>Klanten</h2>
                <i>info klanten, bijvoorbeeld...</i>
                <label>
                    Klant 1
                    <select required>
                    <option value=""> ... </option>
                    <% klanten.forEach(function(klant) { %>
                        <option value="first" > <%= klant.name %> </option>
                    <% }) %>
                    </select>  
                </label>
                <label>
                    Klant 2
                    <select required>
                    <option value=""> ... </option>
                    <% klanten.forEach(function(klant) { %>
                        <option value="first" > <%= klant.name %> </option>
                    <% }) %>
                    </select>  
                </label>
            </section>

            <section>
                <h2>Leveranciers</h2>
                <small>info leveranciers, bijvoorbeeld...</small>
                <label>
                    Leverancier 1
                    <select required>
                    <option value=""> ... </option>
                    </select>  
                </label>
                <label>
                    Leverancier 2
                    <select required>
                    <option value=""> ... </option>
                    </select>  
                </label>
            </section>

            <section>
                <h2>Medewerkers</h2>
                <small>info medewerkers, bijvoorbeeld...</small>
                <label>
                    Medewerker 1
                    <select required>
                    <option value=""> ... </option>
                    </select>  
                </label>
            </section>

            <section>
                <h2>Omgeving</h2>
                <small>info omgeving, bijvoorbeeld...</small>
                <label>
                    Omgeving 1
                    <select required>
                    <option value=""> ... </option>
                    </select>  
                </label>
            </section>
            <button onclick="history.back()">terug</button>
            <button type="submit">volgende</button>
        </fieldset>
    </form>
    
    
    
    
    

    

    

    

    
    
